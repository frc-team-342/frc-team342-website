<%- layout('../layout') -%>

<div class="container">
  <div class="row">

    <div class="col-lg-12">
      <h2 class="page-header">Team 342 <%= capitalize(subteam) %> Schedule 2014</h2>
    </div>
  </div>

  <div class="row">
    <div class="col-lg-12 btn-group btn-group-lg">
    <% for (var index = 0; index < schedules.length; index++) { %>
    <%   var schedule = schedules[index] %>
      <a href="#week-<%= schedule.week %>" type="button" class="btn btn-default">Week <%= schedule.week %></a>
    <% } %>
    </div>
  </div>

  <br>

  <% for (var index = 0; index < schedules.length; index++) { %>
  <%   var schedule = schedules[index] %>
    <% include ./schedule.ejs %>
  <% } %>

</div>

<script src="/js/jquery-1.10.2.js"></script>
<script src="/js/bootstrap.js"></script>
<script src="/js/modern-business.js"></script>